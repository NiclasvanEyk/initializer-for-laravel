{
    "name": "initializer-for-laravel/npm",
    "type": "project",
    "description": "A quickstart generator for Laravel projects.",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "InitializerForLaravel\\Npm\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
